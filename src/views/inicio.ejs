    <!-- ======================== BODY || MAQUETADO DEL MAIN ======================== -->

            <main class="general_main">
                <section class="menu--bottom">
                    <div class="caja_bottom">
                        <a href="../products/imprimir"><img src="/images/imgHome/imgModelosImprimir.jpg" alt="">
                            <p>Modelos para Imprimir</p>
                        </a>
                    </div>

                    <div class="caja_bottom">
                        <a href="../products/disponible"><img src="/images/imgHome/imgModeloStock.jpg" alt="">
                            <p>Modelos Disponibles</p>
                        </a>
                    </div>
                    
                    <div class="caja_bottom">
                        <a <% if (locals.userLogin) { %>
                                    href="../products/personalizado"><img src="/images/imgHome/imgPersonalizado.webp" alt="">
